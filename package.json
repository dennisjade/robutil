  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.5"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "json2csv": "^4.0.2",
